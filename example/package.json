{
  "name": "cdp-api",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env -- ts-node-dev ./src/server.ts",
    "dbm:create": "sequelize migration:create --name",
    "dbm:up": "dotenv -e .env -- sequelize db:migrate --env process_env --config ./sequelize-cli.config.js",
    "dbm:down": "dotenv -e .env -- sequelize db:migrate:undo --env process_env --config ./sequelize-cli.config.js",
    "dbm:reset": "dotenv -e .env -- sequelize db:migrate:undo:all --env process_env --config ./sequelize-cli.config.js",
    "dbm:reup": "npm run dbm:reset && npm run dbm:up",
    "gsa": "dotenv -e .env -- ts-node ./scripts/gsa/index.ts -c ./gsa.config"
  },
  "dependencies": {
    "@apollo/server": "4.5.0",
    "@graphql-tools/graphql-file-loader": "7.5.4",
    "@graphql-tools/load": "7.7.6",
    "bcrypt": "5.0.1",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "graphql": "16.6.0",
    "graphql-parse-resolve-info": "4.13.0",
    "load-envs": "1.0.0",
    "pg": "8.11.3",
    "sequelize": "6.35.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/typed-document-node": "4.0.0",
    "@graphql-codegen/typescript": "3.0.3",
    "@graphql-codegen/typescript-document-nodes": "3.0.3",
    "@graphql-codegen/typescript-operations": "3.0.3",
    "@graphql-codegen/typescript-resolvers": "3.2.0",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.12",
    "@types/fs-extra": "9.0.13",
    "@types/node": "18.16.2",
    "@types/pg": "8.10.9",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "cross-env": "7.0.3",
    "dotenv-cli": "4.0.0",
    "eslint": "7.32.0",
    "sequelize-auto": "https://github.com/kurochin143/sequelize-auto/tarball/build",
    "sequelize-cli": "6.6.2",
    "ts-node": "10.0.0",
    "ts-node-dev": "1.1.8",
    "typescript": "5.0.3"
  }
}
